            <font name="/planeshift/data/ttf/cupandtalon.ttf" r="-1" g="-1" b="-1" size="14" shadow="yes"/>  
        </widget>
        <widget name="label_skill_header" factory="pawsTextBox">
            <frame x="455" y="354" width="40" height="16" border="no" />
            <font name="/planeshift/data/ttf/cupandtalon.ttf" r="11" g="221" b="255" size="14" sr="0" sg="0" sb="0" shadow="yes"/>  
