<widget_description>
    <widget name="Paths" factory="pawsPathWindow" visible="no" movable="no" resizable="no">
        <frame x="0" y="0" width="800" height="600" border="no" />
        <bgimage resource="Paths Background" />
        
        <!-- Title -->
        <widget name="label_paths" factory="pawsTextBox">    
            <frame x="230" y="10" width="350" height="30" border="no"/>
        <font name="/planeshift/data/ttf/cupandtalon.ttf" r="-1" g="-1" b="-1" size="26" shadow="yes"/>
            <text string="PATHS" horizAdjust="CENTRE" />    
        </widget>

        <!-- Path symbol -->
        <widget name="PathSymbol" factory="pawsWidget">
            <frame x="331" y="33" width="137" height="162" />
        </widget>
        
        <widget name="label_parents_header" factory="pawsTextBox">
            <frame x="168" y="116" width="100" height="17" border="no" />
            <font name="/planeshift/data/ttf/cupandtalon.ttf" r="11" g="221" b="255" size="14" shadow="yes"/>  
            <text string="Parents" />
        </widget>
        <widget name="label_parents" factory="pawsMultiLineTextBox">
            <frame x="168" y="147" width="158" height="153" border="no" />
            <font name="/planeshift/data/ttf/cupandtalon.ttf" r="-1" g="-1" b="-1" size="14" shadow="yes"/>  
        </widget>
        <widget name="label_life_header" factory="pawsTextBox">
            <frame x="472" y="116" width="100" height="17" border="no" />
            <font name="/planeshift/data/ttf/cupandtalon.ttf" r="11" g="221" b="255" size="14" shadow="yes"/>  
